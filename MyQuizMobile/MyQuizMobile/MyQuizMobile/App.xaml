<?xml version="1.0" encoding="utf-8"?>

<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:myQuizMobile="clr-namespace:MyQuizMobile"
             x:Class="MyQuizMobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <myQuizMobile:InvertBoolConverter x:Key="InvertBoolConverter" />
            <myQuizMobile:IntStringConverter x:Key="IntStringConverter" />
            <myQuizMobile:ItemTypeToStringConverter x:Key="ItemTypeToStringConverter" />
            <myQuizMobile:IntToBoolConverter x:Key="IntToBoolConverter" />
            <myQuizMobile:DateTimeConverter x:Key="DateTimeConverter" />
            <myQuizMobile:ValueConverterGroup x:Key="InvertIntToBoolConverter">
                <myQuizMobile:IntToBoolConverter />
                <myQuizMobile:InvertBoolConverter />
            </myQuizMobile:ValueConverterGroup>
        </ResourceDictionary>
    </Application.Resources>
</Application>